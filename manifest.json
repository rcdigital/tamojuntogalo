{
  "name": "#tamojuntogalo",
  "version": "1.0.1",
  "description": "No dia mais importante da história do Atlético, até o seu computador vai ficar preto e branco",
  "homepage_url": "http://tamojuntogalo.com.br",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": { 
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Galo-it!",
      "default_icon": "icon.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": ["badge_browser.png"]
}
